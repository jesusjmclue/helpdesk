<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="view_helpdesk_team_timesheet_form" model="ir.ui.view">
        <field name="name">view.helpdesk_team.timesheet.form</field>
        <field name="model">helpdesk.ticket.team</field>
        <field name="inherit_id" ref="helpdesk_mgmt.view_helpdesk_team_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='alias_user_id']" position="after">
                <field name="use_timesheet"/>
            </xpath>
        </field>
    </record>
</odoo>